<Window x:Class="GigavacFuseApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GigavacFuseApp"
        mc:Ignorable="d"
        Title="Sensata | Gigavac" Icon="/Assets/LogoSINTEC S.png" Closing="ClosingWindow" Width="1090" Height="800"
        MinWidth="1050" MinHeight="750" WindowStartupLocation="CenterScreen">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Grid.ColumnSpan="2">
            <Menu Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}">
                <MenuItem Header="_Station" FontSize="13" Margin="5">
                    <MenuItem Name="sbmSettings" Header="_Settings"/>
                    <MenuItem Name="sbmConnect" Header="_Connect" Click="MnuConnectClick"/>
                    <MenuItem Name="sbmDisconnect" Header="_Disconnect" Click="MnuDisconnectClick"/>
                    <Separator/>
                    <MenuItem Header="Exit" Click="MnuExitClick"/>
                </MenuItem>
                <MenuItem Header="_Help" FontSize="13" Margin="5"></MenuItem>
            </Menu>
        </Grid>
        <Grid Grid.Row="1" Grid.ColumnSpan="2">
            <TabControl TabStripPlacement="Left">
                <TabItem Header="Manufactura" Height="40" Width="150" FontSize="15" >
                    <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
                        <TabControl>
                            <TabItem Header="Lot Manager" FontSize="12">
                                <Grid Background="#FFE5E5E5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="62*"/>
                                        <RowDefinition Height="51*"/>
                                        <RowDefinition Height="274.08"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel Grid.Row="0">
                                        <Label Content="Lot Manager" FontSize="20" Foreground="#FF124981" FontWeight="Bold"/>
                                        <Grid Width="792" HorizontalAlignment="Center">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <StackPanel Grid.Column="0" Orientation="Horizontal">
                                                <GroupBox BorderThickness="2" BorderBrush="#FF124981" Width="277" Background="White">
                                                    <GroupBox.Header>
                                                        <Label Content="New lot" Background="#FF124981" Foreground="White" FontSize="13"/>
                                                    </GroupBox.Header>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="80"/>
                                                            <ColumnDefinition Width="*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="*"/>
                                                            <RowDefinition Height="*"/>
                                                            <RowDefinition Height="*"/>
                                                        </Grid.RowDefinitions>
                                                        <Label Grid.Column="0" Grid.Row="0" FontSize="14"
                                                           Content="Device ID:" FontWeight="Bold" VerticalContentAlignment="Center"/>
                                                        <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="0" VerticalAlignment="Center">
                                                            <TextBox Name="dspDeviceID" Background="WhiteSmoke" FontSize="13" Text="Model"
                                                             Width="100" Height="30" HorizontalAlignment="Left" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" Margin="0,2,0,2" VerticalContentAlignment="Center" Padding="5,0,0,0" Focusable="False"/>
                                                            <Button x:Name="btnDeviceID" Width="55" Height="30" FontSize="13"
                                                                    Content="Select" VerticalAlignment="Center" Margin="5,0,0,0" PreviewMouseLeftButtonDown="BtnDeviceIDPress" PreviewMouseLeftButtonUp="BtnDeviceIDRelease"/>
                                                            <Rectangle Name="lmpDeviceID" HorizontalAlignment="Left" Height="30" 
                                                                       Margin="1,0,0,0" VerticalAlignment="Center" Width="15" 
                                                                       StrokeThickness="1">
                                                                <Rectangle.Fill>
                                                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                        <GradientStop Color="Gray" Offset="1"/>
                                                                        <GradientStop Color="White" Offset="0.481"/>
                                                                        <GradientStop Color="Gray" Offset="0"/>
                                                                    </LinearGradientBrush>
                                                                </Rectangle.Fill>
                                                            </Rectangle>
                                                        </StackPanel>
                                                        <Label Grid.Column="0" Grid.Row="1" FontSize="15"
                                                           Content="Lot ID:" FontWeight="Bold" VerticalAlignment="Center"/>
                                                        <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="1" VerticalAlignment="Center">
                                                            <TextBox Name="dspLotID" FontSize="13" Text="Lot ID" Background="WhiteSmoke"
                                                             Width="100" Height="30" HorizontalAlignment="Left" BorderBrush="Black" HorizontalContentAlignment="Right"
                                                             Margin="0,2,0,2" IsEnabled="True" Focusable="False" VerticalContentAlignment="Center" Padding="5,0,0,0"/>
                                                            <Button x:Name="btnLotID" Width="55" Height="30" FontSize="13"
                                                                    Content="Select" Margin="5,0,0,0" PreviewMouseLeftButtonDown="BtnLotIDPress" PreviewMouseLeftButtonUp="BtnLotIDRelease"/>
                                                            <Rectangle Name="lmpLotID" HorizontalAlignment="Left" Height="30" 
                                                                       Margin="1,0,0,0" VerticalAlignment="Center" Width="15" 
                                                                       StrokeThickness="1">
                                                                <Rectangle.Fill>
                                                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                        <GradientStop Color="Gray" Offset="1"/>
                                                                        <GradientStop Color="White" Offset="0.481"/>
                                                                        <GradientStop Color="Gray" Offset="0"/>
                                                                    </LinearGradientBrush>
                                                                </Rectangle.Fill>
                                                            </Rectangle>
                                                        </StackPanel>
                                                        <Label Grid.Column="0" Grid.Row="2"  FontSize="15"
                                                           Content="Cantidad:" FontWeight="Bold" VerticalAlignment="Center"/>
                                                        <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="2">
                                                            <TextBox Grid.Column="1" Grid.Row="2" FontSize="13" Text="0" Background="WhiteSmoke"
                                                               Name="dspQty" HorizontalContentAlignment="Right"
                                                               Width="100" Height="30" HorizontalAlignment="Left" BorderBrush="Black"
                                                               Margin="0,2,0,2" IsEnabled="True" Focusable="False" VerticalContentAlignment="Center" Padding="5,0,0,0"/>
                                                            <Button x:Name="btnQty" Width="55" Height="30" FontSize="13"
                                                                    Content="Select" Margin="5,0,0,0" PreviewMouseLeftButtonDown="BtnQtyPress" PreviewMouseLeftButtonUp="BtnQtyRelease"/>
                                                            <Rectangle Name="lmpQty" HorizontalAlignment="Left" Height="30" 
                                                                       Margin="1,0,0,0" VerticalAlignment="Center" Width="15" 
                                                                       StrokeThickness="1">
                                                                <Rectangle.Fill>
                                                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                        <GradientStop Color="Gray" Offset="1"/>
                                                                        <GradientStop Color="White" Offset="0.481"/>
                                                                        <GradientStop Color="Gray" Offset="0"/>
                                                                    </LinearGradientBrush>
                                                                </Rectangle.Fill>
                                                            </Rectangle>
                                                        </StackPanel>
                                                    </Grid>
                                                </GroupBox>
                                                <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                                                    <Button Name="btnNewLot" Width="80" Height="40" Margin="10" Content="New" BorderBrush="DarkOliveGreen" BorderThickness="2" Click="BtnNewLot"/>
                                                    <Button Name="btnEndLot" Width="80" Height="40" Margin="10" Content="End" BorderBrush="DarkRed" BorderThickness="2"/>
                                                </StackPanel>
                                            </StackPanel>
                                            <GroupBox Grid.Column="1" BorderThickness="2" BorderBrush="#FF811E12" Background="White" HorizontalAlignment="Center" Width="265">
                                                <GroupBox.Header>
                                                    <Label Content="Current lot" Background="#FF811E12" Foreground="White" FontSize="13"/>
                                                </GroupBox.Header>
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="90"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <Label Grid.Column="0" Grid.Row="0" FontSize="15"
                                                           Content="Device ID:" FontWeight="Bold" VerticalAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="Device ID"
                                                     Name="dspSelectedDvcID" Background="WhiteSmoke"
                                                     Width="100" Height="30" HorizontalAlignment="Left"
                                                     BorderBrush="Black" Margin="0,2,0,2" IsEnabled="True" Focusable="False"
                                                     VerticalContentAlignment="center" HorizontalContentAlignment="Right"/>
                                                    <Label Grid.Column="0" Grid.Row="1" FontSize="15"
                                                   Content="Lot ID:" FontWeight="Bold" VerticalAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="1" FontSize="13" Text="Lot ID"
                                                     Name="dspSelectedLotID" Background="WhiteSmoke"
                                                     Width="100" HorizontalAlignment="Left" BorderBrush="Black"
                                                     Margin="0,2,0,2" IsEnabled="True" Focusable="False"
                                                     VerticalContentAlignment="Center" HorizontalContentAlignment="Right"/>
                                                    <Label Grid.Column="0" Grid.Row="2" 
                                                   Content="Cantidad:" FontWeight="Bold" FontSize="15"
                                                   VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="2" FontSize="13" Text="Quantity"
                                                     Name="dspSelectedQty" Background="WhiteSmoke"
                                                     Width="100" HorizontalAlignment="Left" BorderBrush="Black"
                                                     Margin="0,2,0,2" IsEnabled="True" Focusable="False"
                                                     VerticalContentAlignment="Center" HorizontalContentAlignment="Right"/>
                                                </Grid>
                                            </GroupBox>

                                        </Grid>
                                    </StackPanel>
                                    <Grid Grid.Row="1" Margin="0,9,0,0">
                                        <GroupBox BorderThickness="2" BorderBrush="#FF124981" Height="130" VerticalAlignment="Top" HorizontalAlignment="Center" Width="840">
                                            <GroupBox.Header>
                                                <Label Content="SQL Database Info." Background="#FF124981" Foreground="White" FontSize="13"/>
                                            </GroupBox.Header>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid Grid.Column="0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="110*"/>
                                                        <ColumnDefinition Width="153*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Label Grid.Column="0" Grid.Row="0" FontSize="14"
                                                           Content="Server name:" FontWeight="Normal" VerticalContentAlignment="Center"
                                                           VerticalAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Name="dspServer" Background="White" FontSize="13" Text="Server name"
                                                             Width="132" HorizontalAlignment="Center" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" VerticalContentAlignment="Center" 
                                                             Padding="0,0,0,0" Focusable="False" Height="30" VerticalAlignment="Center"/>
                                                    <Label Grid.Column="0" Grid.Row="1" FontSize="14" VerticalAlignment="Center"
                                                           Content="Database name:" FontWeight="Normal" VerticalContentAlignment="Center" Grid.ColumnSpan="2" Height="28" Margin="0,0,151,0"/>
                                                    <TextBox Grid.Column="1" Grid.Row="1" Name="dspDatabase" Background="White" FontSize="13" Text="Database name"
                                                             Width="132" HorizontalAlignment="Center" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" VerticalContentAlignment="Center" 
                                                             Padding="5,0,0,0" Focusable="False" Height="30" VerticalAlignment="Center"/>
                                                </Grid>
                                                <Grid Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="110*"/>
                                                        <ColumnDefinition Width="144*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Label Grid.Column="0" Grid.Row="0" FontSize="14"
                                                           Content="Machine name:" FontWeight="Normal" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" Name="dspMchnName" Background="White" FontSize="13" Text="Machine name"
                                                             Width="132" HorizontalAlignment="Center" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" VerticalContentAlignment="Center" 
                                                             Padding="0,0,0,0" Focusable="False" Height="30" VerticalAlignment="Center"/>
                                                    <Label Grid.Column="0" Grid.Row="1" FontSize="14" VerticalAlignment="Center"
                                                           Content="Machine No.:" FontWeight="Normal" VerticalContentAlignment="Center" Height="28"/>
                                                    <TextBox Grid.Column="1" Grid.Row="1" Name="dspMchnNo" Background="White" FontSize="13" Text="Machine no."
                                                             Width="132" HorizontalAlignment="Center" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" VerticalContentAlignment="Center" 
                                                             Padding="0,0,0,0" Focusable="False" Height="30" VerticalAlignment="Center"/>
                                                </Grid>
                                                <Grid Grid.Column="2">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="115*"/>
                                                        <ColumnDefinition Width="144*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Label Grid.Column="0" Grid.Row="0" FontSize="14" VerticalAlignment="Center"
                                                           Content="Machine ID:" FontWeight="Normal" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" Name="dspMchnID" Background="White" FontSize="13" Text="Machine ID"
                                                             Width="132" Height="30" HorizontalAlignment="Center" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" VerticalContentAlignment="Center" 
                                                             Padding="0,0,0,0" Focusable="False"/>
                                                    <Label Grid.Column="0" Grid.Row="1" FontSize="14" VerticalAlignment="Center"
                                                           Content="Machine No.:" FontWeight="Normal" VerticalContentAlignment="Center" Height="28" Margin="0,0,12,0"/>
                                                    <TextBox Grid.Column="1" Grid.Row="1" Name="dspEqmntID" Background="White" FontSize="13" Text="Machine no."
                                                             Width="132" HorizontalAlignment="Center" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" VerticalContentAlignment="Center" 
                                                             Padding="0,0,0,0" Focusable="False" Height="30" VerticalAlignment="Center"/>
                                                    <Label Grid.Column="0" Grid.Row="2" FontSize="14" VerticalAlignment="Center"
                                                           Content="Assig/Check ID.:" FontWeight="Normal" VerticalContentAlignment="Center" Height="28" Margin="0,0,12,0"/>
                                                    <TextBox Grid.Column="1" Grid.Row="2" Name="dspChckID" Background="White" FontSize="13" Text="Check ID"
                                                             Width="132" HorizontalAlignment="Center" HorizontalContentAlignment="Right"
                                                             BorderBrush="Black" VerticalContentAlignment="Center" 
                                                             Padding="0,0,0,0" Focusable="False" Height="30" VerticalAlignment="Center"/>
                                                </Grid>
                                            </Grid>
                                        </GroupBox>
                                    </Grid>
                                    <Grid Grid.Row="2" Margin="0,12,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                    </Grid>
                                    <Grid Grid.Row="3">
                                        <TabControl BorderBrush="#FF124981" BorderThickness="2" Height="254" 
                                                    VerticalAlignment="Center" HorizontalAlignment="Center" Width="840">
                                            <TabItem Header="Log" BorderBrush="#FF124981" BorderThickness="2">
                                                <ScrollViewer VerticalScrollBarVisibility="Auto" >
                                                    <StackPanel Name="stpDBMsg"></StackPanel>
                                                </ScrollViewer>
                                            </TabItem>
                                            <TabItem Header="Common" BorderBrush="#FF124981" BorderThickness="2">
                                                <TabControl>
                                                    <TabItem Header="Table">
                                                        <Grid>
                                                            <DataGrid x:Name="grdCommonTbl" ItemsSource="{Binding}" AutoGenerateColumns="True"/>
                                                        </Grid>
                                                    </TabItem>
                                                    <TabItem Header="Master">
                                                        <Grid>
                                                            <DataGrid x:Name="grdCommonMstr"/>
                                                        </Grid>
                                                    </TabItem>
                                                </TabControl>
                                            </TabItem>
                                            <TabItem Header="Install shield" BorderBrush="#FF124981" BorderThickness="2">
                                                <TabControl>
                                                    <TabItem Header="Table">
                                                        <Grid>
                                                            <DataGrid x:Name="grdShldTbl" ItemsSource="{Binding}" AutoGenerateColumns="True"/>
                                                        </Grid>
                                                    </TabItem>
                                                    <TabItem Header="Master">
                                                        <Grid>
                                                            <DataGrid x:Name="grdShldMstr"/>
                                                        </Grid>
                                                    </TabItem>
                                                </TabControl>
                                            </TabItem>
                                            <TabItem Header="Install Nut" BorderBrush="#FF124981" BorderThickness="2">
                                                <TabControl>
                                                    <TabItem Header="Table">
                                                        <Grid>
                                                            <DataGrid x:Name="grdNutTbl" ItemsSource="{Binding}" AutoGenerateColumns="True"/>
                                                        </Grid>
                                                    </TabItem>
                                                    <TabItem Header="Master">
                                                        <Grid>
                                                            <DataGrid x:Name="grdNutMstr"/>
                                                        </Grid>
                                                    </TabItem>
                                                </TabControl>
                                            </TabItem>
                                            <TabItem Header="Plunger &amp; Armature Assy" BorderBrush="#FF124981" BorderThickness="2">
                                                <TabControl>
                                                    <TabItem Header="Table">
                                                        <Grid>
                                                            <DataGrid x:Name="grdPlngrTbl" ItemsSource="{Binding}" AutoGenerateColumns="True"/>
                                                        </Grid>
                                                    </TabItem>
                                                    <TabItem Header="Master">
                                                        <Grid>
                                                            <DataGrid x:Name="grdPlngrMstr"/>
                                                        </Grid>
                                                    </TabItem>
                                                </TabControl>
                                            </TabItem>
                                            <TabItem Header="Latch Assy" BorderBrush="#FF124981" BorderThickness="2">
                                                <TabControl>
                                                    <TabItem Header="Table">
                                                        <Grid>
                                                            <DataGrid x:Name="grdLatchTbl" ItemsSource="{Binding}" AutoGenerateColumns="True"/>
                                                        </Grid>
                                                    </TabItem>
                                                    <TabItem Header="Master">
                                                        <Grid>
                                                            <DataGrid x:Name="grdLatchMstr"/>
                                                        </Grid>
                                                    </TabItem>
                                                </TabControl>
                                            </TabItem>
                                        </TabControl>
                                    </Grid>
                                </Grid>
                            </TabItem>
                            <TabItem Header="Monitor" FontSize="12">
                                <Grid Background="#FFE5E5E5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Column="0" Height="613" VerticalAlignment="Center" HorizontalAlignment="Right" Width="215">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="277*"/>
                                            <RowDefinition Height="336*"/>
                                        </Grid.RowDefinitions>
                                        <GroupBox Grid.Row="0" BorderThickness="2" BorderBrush="#FF124981" Width="200" 
                                              Background="White" HorizontalAlignment="Center" Margin="0,15,0,10">
                                            <GroupBox.Header>
                                                <Label Content="Terminal shield limits" Background="#FF124981" 
                                                   Foreground="White" FontSize="13"/>
                                            </GroupBox.Header>
                                            <Grid>
                                                <DataGrid x:Name="grdShldLimits" ItemsSource="{Binding}" AutoGenerateColumns="True" IsReadOnly="False"/>
                                            </Grid>
                                        </GroupBox>
                                        <TabControl Grid.Row="1" HorizontalAlignment="Center" Width="205">
                                            <TabItem Header="Prior OP Check">
                                                <StackPanel>
                                                    <Label Content="Check components" Height="25" Padding="0" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="dspShldChckComp"
                                                             Name="dspShldChckComp" Background="WhiteSmoke"
                                                             Width="195" Height="25" HorizontalAlignment="Center"
                                                             BorderBrush="Black" IsEnabled="True" Focusable="False"
                                                             VerticalContentAlignment="center" HorizontalContentAlignment="Left"/>
                                                    <Grid Height="130" Margin="5">
                                                        <ScrollViewer VerticalScrollBarVisibility="Auto">
                                                            <DataGrid x:Name="grdShldPriorOp" ItemsSource="{Binding}" IsReadOnly="True"/>
                                                        </ScrollViewer>
                                                    </Grid>
                                                    <Label Content="Assing components" Height="25" Padding="0" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="dspShldAssingComp"
                                                             Name="dspShldAssingComp" Background="WhiteSmoke"
                                                             Width="195" Height="25" HorizontalAlignment="Center"
                                                             BorderBrush="Black" IsEnabled="True" Focusable="False"
                                                             VerticalContentAlignment="center" HorizontalContentAlignment="Left"/>
                                                    <Label Content="Update device status" Height="25" Padding="0" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="dspShldAssingComp"
                                                             Name="dspShldUpdtDvcSts" Background="WhiteSmoke"
                                                             Width="195" Height="25" HorizontalAlignment="Center"
                                                             BorderBrush="Black" IsEnabled="True" Focusable="False"
                                                             VerticalContentAlignment="center" HorizontalContentAlignment="Left"/>
                                                </StackPanel>
                                            </TabItem>
                                            <TabItem Header="Results">
                                                <Grid>
                                                    <DataGrid x:Name="grdShldRslt" ItemsSource="{Binding}" IsReadOnly="True"/>
                                                </Grid>
                                            </TabItem>
                                        </TabControl>
                                    </Grid>
                                    <Grid Grid.Column="1" Height="613" VerticalAlignment="Center" HorizontalAlignment="Right" Width="215">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="277*"/>
                                            <RowDefinition Height="336*"/>
                                        </Grid.RowDefinitions>
                                        <GroupBox Grid.Row="0" BorderThickness="2" BorderBrush="#FF124981" Width="200" 
                                              Background="White" HorizontalAlignment="Center" Margin="0,15,0,10">
                                            <GroupBox.Header>
                                                <Label Content="Nut inserts limits" Background="#FF124981" 
                                                   Foreground="White" FontSize="13"/>
                                            </GroupBox.Header>
                                            <Grid>
                                                <DataGrid x:Name="grdNutLimits" ItemsSource="{Binding}" AutoGenerateColumns="True" IsReadOnly="False"/>
                                            </Grid>
                                        </GroupBox>
                                        <TabControl Grid.Row="1" HorizontalAlignment="Center" Width="205">
                                            <TabItem Header="Prior OP Check">
                                                <StackPanel>
                                                    <Label Content="Check components" Height="25" Padding="0" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="dspNutChckComp"
                                                             Name="dspNutChckComp" Background="WhiteSmoke"
                                                             Width="195" Height="25" HorizontalAlignment="Center"
                                                             BorderBrush="Black" IsEnabled="True" Focusable="False"
                                                             VerticalContentAlignment="center" HorizontalContentAlignment="Left"/>
                                                    <Grid Height="130" Margin="5">
                                                        <ScrollViewer VerticalScrollBarVisibility="Auto">
                                                            <DataGrid x:Name="grdNutPriorOp" ItemsSource="{Binding}" IsReadOnly="True"/>
                                                        </ScrollViewer>
                                                    </Grid>
                                                    <Label Content="Assing components" Height="25" Padding="0" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="dspNutAssingComp"
                                                             Name="dspNutAssingComp" Background="WhiteSmoke"
                                                             Width="195" Height="25" HorizontalAlignment="Center"
                                                             BorderBrush="Black" IsEnabled="True" Focusable="False"
                                                             VerticalContentAlignment="center" HorizontalContentAlignment="Left"/>
                                                    <Label Content="Update device status" Height="25" Padding="0" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="dspNutAssingComp"
                                                             Name="dspNutUpdtDvcSts" Background="WhiteSmoke"
                                                             Width="195" Height="25" HorizontalAlignment="Center"
                                                             BorderBrush="Black" IsEnabled="True" Focusable="False"
                                                             VerticalContentAlignment="center" HorizontalContentAlignment="Left"/>
                                                </StackPanel>
                                            </TabItem>
                                            <TabItem Header="Results">
                                                <Grid>
                                                    <DataGrid x:Name="grdNutRslt" ItemsSource="{Binding}" IsReadOnly="True"/>
                                                </Grid>
                                            </TabItem>
                                        </TabControl>
                                    </Grid>
                                    <Grid Grid.Column="2" Height="613" VerticalAlignment="Center" HorizontalAlignment="Right" Width="215">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="277*"/>
                                            <RowDefinition Height="336*"/>
                                        </Grid.RowDefinitions>
                                        <GroupBox Grid.Row="0" BorderThickness="2" BorderBrush="#FF124981" Width="200" 
                                              Background="White" HorizontalAlignment="Center" Margin="0,15,0,10">
                                            <GroupBox.Header>
                                                <Label Content="Plunger &amp; armature limits" Background="#FF124981" 
                                                   Foreground="White" FontSize="13"/>
                                            </GroupBox.Header>
                                            <Grid>
                                                <DataGrid x:Name="grdPlngrLimits" ItemsSource="{Binding}" AutoGenerateColumns="True" IsReadOnly="False"/>
                                            </Grid>
                                        </GroupBox>
                                        <TabControl Grid.Row="1" HorizontalAlignment="Center" Width="205">
                                            <TabItem Header="Prior OP Check">
                                                <StackPanel>
                                                    <Label Content="Check components" Height="25" Padding="0" VerticalContentAlignment="Center"/>
                                                    <TextBox Grid.Column="1" Grid.Row="0" FontSize="13" Text="dspPlngrChckComp"
